{
    "plugins": ["only-warn"],
    "extends": ["airbnb", "prettier"],
    "parser": "babel-eslint",
    "env": {
      "jest": true
    },
    "globals": {
      "window": true,
      "document": true
    },
    "rules": {
      "no-console": "off",
      "react/state-in-constructor": [0, 0],
     "import/no-unresolved": 1,
     "react/jsx-filename-extension": [
      1,
      {
         "extensions": [
           ".js"
        ] 
      }
    ]
    },
    "settings": {
      "import/resolver": {
        "node": {
          "paths": ["src"],
          "extensions": [".js",".jsx",".ts","tsx"]
        }
      }
    }
  }
  